spring:
 application:
  name: gateway
 
 cloud:
  loadbalancer:
   enabled: true

  gateway:
   server:
    webflux:
     routes:
      - id: msv-medicos
        uri: lb://msv-medicos
        predicates:
         - Path=/api/medicos/**
        filters:
         - StripPrefix=2

      - id: msv-pacientes
        uri: lb://msv-pacientes
        predicates:
         - Path=/api/pacientes/**
        filters:
         - StripPrefix=2

 
server:
 port: 8090

eureka:
 client:
  service-url:
   defaultZone: http://localhost:8761/eureka